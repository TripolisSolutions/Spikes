<h1>Listing twitter_accounts</h1>
<div class="notice" ><%= notice %></div>
<table>
  <tr>
    <th></th>
    <th></th>
    <th></th>

    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @twitter_accounts.each do |twitter_account| %>
  <tr>
    <td><%= twitter_account.id %></td>
    <td><%= twitter_account.username %></td>
    <td><%= twitter_account.full_name %></td>

    <td><%= link_to 'Show', twitter_account ,{"data-pjax" =>'#main'}%></td>

    <td><%= link_to 'Destroy', twitter_account, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    <td><%= link_to 'Edit', edit_twitter_account_path(twitter_account.id) %></td>
  </tr>
<% end %>
</table>

